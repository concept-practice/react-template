{
	"overrides": [
		{
			"files": ["**/*.ts?(x)"],
			"extends": ["react-app", "react-app/jest", "prettier", "plugin:prettier/recommended"],
			"rules": {
				"@typescript-eslint/explicit-function-return-type": [1],
				"react/self-closing-comp": [
					1,
					{
						"component": true,
						"html": true
					}
				],
				"@typescript-eslint/no-unused-vars": [1]
			}
		}
	]
}
